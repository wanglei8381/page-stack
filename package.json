  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -w -c rollup.config.dev.js",
    "lint": "eslint src",
    "test": "jest"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.9.0",
    "jest": "^21.2.1",
    "rollup": "^0.50.0",
    "rollup-plugin-buble": "^0.16.0",
    "vue": "^2.5.2"
  },
  "jest": {
    "testRegex": "/test/index.js",
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
